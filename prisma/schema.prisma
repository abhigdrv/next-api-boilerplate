datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  username String?
  mobile   String? @unique
  password String?
  approved Boolean @default(false)
}
